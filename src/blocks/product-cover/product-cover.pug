+e.wrap
	if data.image
		+e.image(style=`background-image:url("${data.image}");`)
	if data.video
		+e.video
+e.content
	if data.title
		+e.title!= data.title
	if data.slider
		+e.slider-wrapper
			+e.slider-text
				+e.text-vertical!= data.slider.verticalTxt
				+e.text!= data.slider.noteTxt
			+e.slider
				if data.slider.bottleImage
					+e.slider-inner
						each image, index in data.slider.bottleImage
							+e.slider-item
								+e("img").slider-image(src=image, alt="")
+e.close
