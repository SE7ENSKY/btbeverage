fit-image()
	text-align center
	&:after
		content ""
		display inline-block
		vertical-align middle
		height 100%
		width 0
	>img
		display inline-block
		vertical-align middle
		max-width 100%
		max-height 100%

iconParams()
	font-family 'icons'
	speak none
	font-style normal
	font-weight normal
	font-variant normal
	text-transform none
	line-height 1
	-webkit-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale
