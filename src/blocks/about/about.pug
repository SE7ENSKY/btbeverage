+e.inner.container
	+e("h1").title
		+e("span").title-row._1 We brew
		+e("span").title-row._2 Kombucha
		+e("span").title-row._3 the right way
		+e("img").title-image#leaf(src=i("leaf.png"), alt="")
	+e.list.first
		+e.list-item
			+e.list-image
				+e("img").list-image-i(src=i("ingridient-icon-1.svg"), alt="" width="65" height="65")
			+e.list-title Darjeeling Black Tea or<br>Japanese Bancha Green Tea
		+e.list-separator
		+e.list-item._second-item
			+e.list-image
				+e("img").list-image-i(src=i("ingridient-icon-2.svg"), alt="" width="65" height="65")
			+e.list-title Organic Cane Sugar
		+e.bottle
			+e("img").bottle-i(src=i('bottle.png'), alt="")
	+e.list
		+e.list-item
			+e.list-image
				+e("img").list-image-i(src=i("ingridient-icon-3.svg"), alt="" width="65" height="65")
			+e.list-title Pilsner Laden Water
		+e.list-separator
		+e.list-item._second-item
			+e.list-image
				+e("img").list-image-i(src=i("ingridient-icon-4.svg"), alt="" width="65" height="65")
			+e.list-title Natural Flavor &amp; Herbs
		
