+e.wrap
	+e.wrap-inner
		if data.image
			+e.image(style=`background-image:url("${data.image}");`)
		if data.video
			+e.video-wrapper
				+e("video").video(data-video=data.video data-type="video/mp4" preload="auto" loop muted)
+e("a").content(href=data.externalLink target="_blank")
	if data.title
		+e.title!= data.title
