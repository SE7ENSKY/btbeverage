+e.wrap
	if data.image
		+e.image-wrapper
			+e.image(style=`background-image:url("${data.image}");`)
	if data.video
		+e.video-wrapper
			+e("video").video(data-video=data.video type="video/mp4" preload="preload")
+e.content
	if data.title
		+e.title!= data.title
	if data.slider
		+e.slider-wrapper
			+e.slider-text
				+e.text-vertical!= data.slider.verticalTxt
				+e.text!= data.slider.noteTxt
			+e.slider
				if data.slider.bottleImage
					+e.slider-inner
						each image, index in data.slider.bottleImage
							- classes = []
							if index == 0
								- classes.push("active")
							+e.slider-item(class=classes)
								+e("img").slider-image(src=image, alt="")
+e.close
